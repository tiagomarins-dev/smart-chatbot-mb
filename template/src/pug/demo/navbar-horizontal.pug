extends ../index



append variables
  - var attrsHtml = Object.assign(attrsHtml, { 'data-navigation-type': "horizontal", 'data-navbar-horizontal-shape': "default" })
  - var disableNavigationType = true;
  - var disableVerticalNavbarAppearance = true;
  - var disableHorizontalNavbarShape = true;

append headScripts
  if ENV === 'PROD'
    script.
      window.config.set({
        phoenixNavbarPosition: 'horizontal',
        phoenixNavbarTopShape: 'default',
      });

block navbarVertical

block navbarTop
  +NavbarTop({
    toggleID: 'navbarTopCollapse',
    links: false,
    search: true
    }).navbar-expand-lg

block afterWrapperContent